{
    "name": "languara/laravel",
    "keywords": ["Languara", "Laravel", "Translation", "Localization", "Localize", "Internationalization", "Internationalize", "Translate", "l10n", "i18n"],
    "homepage": "https://languara.com/localization/laravel",
    "description": "Languara localization plugin for Laravel 4.x. Easily upload, translate, and synchronize your language content using Languara's Cloud Based Translation Management Platform.",
    "license": "MIT",
    "authors": [
        {
            "name": "Languara Development Team",
            "email": "support@languara.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "languara/core": "~1.0"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Languara\\Laravel\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
